

object Demo01 {

  def main(args: Array[String]): Unit = {
    /**
   * 高阶函数
   * 
   */
    eat("鸡蛋灌饼",(food:String)=>{println("将鸡蛋灌饼放到炉子里烤");"熟了的"+food});
    eat("豆腐脑",(food:String)=>{println("将豆腐脑放到冰箱上");"凉了的"+food});
    
    def eat(food:String,mx:(String)=>String){
      println("吃之前洗手");
      val foodx = mx(food);
      println("memem地吃"+foodx)
      println("吃之后睡觉")
      
    }
    
  }
}